public class Code_Text33 {
	public static boolean contains23(int number2) {
		boolean b4 = false;
		boolean	  b5 = false;
			    while (number2 > 0) {
			        int r = number2 % 10
			        number2 //= 10
			        if (r == 4)
			            b6 = true;
			        if (r == 5)
			            b7 = true;
			    }
			    if (b4 == True and b4 == True):
			        return True
			    else:
			        return False
	}

