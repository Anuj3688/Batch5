package com.cwa.learn1;

public class Learn {
	private String name;
	private int rollno;
	
	Learn(){
		
	}
	Learn(String name,int rollno){
		this.name=name;
		this.rollno = rollno;
	}
}

class Driver{
	
	public static void main(String[] args) {
		Learn l1 = new Learn();
		System.out.println(l1.name);
	}
}
