package com.cwa.learn1;

import java.util.Stack;

public class Stack_Reverse {
	public static void main(String[] args) {
		Stack<Character> a = new Stack<>();
		String a1 = "Working";
		for(int i=0 ; i<a.length() ; i++) {
			a.push(a1.charAt(i));
		}
		String ans = "";
		for(int i=0 ; i<a.length() ; i++) {
			ans += a.pop();
		}
		System.out.println(ans);
	}
}
